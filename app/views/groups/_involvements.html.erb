<div id="involvements">
  <% if @involvements.length > 0 %>
    <table> 
    <tr> 
      <th>Name</th> 
      <th>Role</th> 
      <th>Technology</th> 
      <th>Start Date</th> 
      <th>End Date</th>
      <th></th> 
    </tr> 
    <% @involvements.each do |i| %>
      <tr>
      <td><%=i.user.nickname%></td>      
      <td><%="#{i.status} #{i.role.name}"%></td>      
      <td><%=i.role.tech%></td>      
      <td><%=i.start_date%></td>      
      <td><%=i.end_date%></td>      
      <td>
        <% if i.user == current_user %>
          <%= form_for i, :html => {:method => :delete}, :remote => true do |f| %>
          <%= f.submit "Delete" %>    
          <% end %>
        <% end %>
      </td>
      </tr>      
    <% end %>
    </table>
  <% end %>
</div>  
